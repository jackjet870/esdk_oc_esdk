<?xml version="1.0" encoding="UTF-8"?>
<service-config>
	<service_template>
		<id>demo-apply-volume</id>
		<name>demo_applyCloudDisk_button</name>
		<description></description>
		<type>apply</type>
		<view_type>system</view_type>
		<process_definition_key>esdk.demo.ApplyVolumeProcess</process_definition_key>
		<templateUrl>service.createEsdkServiceStorage</templateUrl>
		<modifyUrl>service.createEsdkServiceStorage</modifyUrl>
		<applyUrl>ssp.applyEsdkDisk</applyUrl>
		<approvalUrl>ssp.approvalEsdkDiskApply</approvalUrl>
		<imageUrl>/demo/diskService.jpg</imageUrl>
		<no_approve>true</no_approve>
		<vdc_approve>true</vdc_approve>
		<domain_approve>false</domain_approve>
		<vdc_domain_approve>false</vdc_domain_approve>
	</service_template>
	<service_template>
		<id>demo-modify-volume</id>
		<name></name>
		<description></description>
		<type>modify</type>
		<view_type>system</view_type>
		<process_definition_key></process_definition_key>
		<templateUrl></templateUrl>
		<modifyUrl></modifyUrl>
		<applyUrl>ssp.changeEsdkDisk</applyUrl>
		<approvalUrl>ssp.approvalDiskChange</approvalUrl>
		<imageUrl></imageUrl>
		<no_approve>true</no_approve>
		<vdc_approve>true</vdc_approve>
		<domain_approve>false</domain_approve>
		<vdc_domain_approve>false</vdc_domain_approve>
	</service_template>
	<service_template>
		<id>demo-extend-volume</id>
		<name></name>
		<description></description>
		<type>extend</type>
		<view_type>system</view_type>
		<process_definition_key>service.defer.ExtendServiceProcess</process_definition_key>
		<templateUrl></templateUrl>
		<modifyUrl></modifyUrl>
		<applyUrl>ssp.applyInstanceExtend</applyUrl>
		<approvalUrl>ssp.approvalInstanceExtend</approvalUrl>
		<imageUrl></imageUrl>
		<no_approve>true</no_approve>
		<vdc_approve>true</vdc_approve>
		<domain_approve>false</domain_approve>
		<vdc_domain_approve>false</vdc_domain_approve>
	</service_template>
	<service_template>
		<id>demo-decommission-volume</id>
		<name></name>
		<description></description>
		<type>release</type>
		<view_type>system</view_type>
		<process_definition_key></process_definition_key>
		<templateUrl></templateUrl>
		<modifyUrl></modifyUrl>
		<applyUrl></applyUrl>
		<approvalUrl>ssp.viewInstanceRelease</approvalUrl>
		<imageUrl></imageUrl>
		<no_approve>true</no_approve>
		<vdc_approve>true</vdc_approve>
		<domain_approve>false</domain_approve>
		<vdc_domain_approve>false</vdc_domain_approve>
	</service_template>
	
	<service_templates_group>
		<id>demo-service-templates-group-vol</id>
		<name>demo-Volume</name>
		<apply_template_id>demo-apply-volume</apply_template_id>
		<modify_template_id>demo-modify-volume</modify_template_id>
		<extend_template_id>demo-extend-volume</extend_template_id>
		<release_template_id>demo-decommission-volume</release_template_id>
	</service_templates_group>
</service-config>