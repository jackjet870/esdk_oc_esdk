		<plugins>
			<!-- checkstyle -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-checkstyle-plugin</artifactId>
				<version>2.6</version>
				<executions>
					<execution>
						<phase>site</phase>
						<goals>
							<goal>checkstyle</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<configLocation>
						hwrulesets/checkstyle/checkstyle_ruleset_minimal.xml</configLocation>
					<suppressionsLocation>
						hwrulesets/checkstyle/checkstyle-suppressions.xml</suppressionsLocation>
					<violationSeverity>warning</violationSeverity>
					<excludes>${excludesFile}</excludes>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>com.huawei.ci.maven</groupId>
						<artifactId>ci-ruleset</artifactId>
						<version>${com.huawei.ci.ruleset.version}</version>
					</dependency>
				</dependencies>
			</plugin>
			<!-- findbugs -->
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>findbugs-maven-plugin</artifactId>
				<version>2.3.2</version>
				<executions>
					<execution>
						<phase>site</phase>
						<goals>
							<goal>findbugs</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<findbugsXmlOutput>true</findbugsXmlOutput>
					<effort>Max</effort>
					<threshold>Low</threshold>
					<!-- 2.5 hours -->
					<timeout>9000000</timeout>
					<maxHeap>1024</maxHeap>
					<omitVisitors></omitVisitors>
					<visitors></visitors>
					<excludeFilterFile>
						findbugs-exclude.xml</excludeFilterFile>
					<excludes>${excludesFile}</excludes>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>com.huawei.ci.findbugs</groupId>
						<artifactId>findbugs</artifactId>
						<version>1.3.9</version>
					</dependency>
					<dependency>
						<groupId>com.google.code.findbugs</groupId>
						<artifactId>findbugs-ant</artifactId>
						<version>1.3.9</version>
						<exclusions>
							<exclusion>
								<groupId>com.google.code.findbugs</groupId>
								<artifactId>findbugs</artifactId>
							</exclusion>
						</exclusions>
					</dependency>
				</dependencies>
			</plugin>
			<!-- CIFilterPlugin -->
			<plugin>
				<groupId>com.huawei.ci</groupId>
				<artifactId>maven-cifilter-plugin</artifactId>
				<version>1.0.0</version>
				<configuration>
					<excludes>
						<exclude>${excludesFile}</exclude>
					</excludes>
				</configuration>
			</plugin>
			<!-- simian -->
			<plugin>
				<groupId>com.huawei.ci.maven.plugins</groupId>
				<artifactId>maven-simian-plugin</artifactId>
				<version>1.0</version>
				<configuration>
					<threshold>10</threshold>
					<excludes>${excludesFile}</excludes>
				</configuration>
				<executions>
					<execution>
						<phase>site</phase>
						<goals>
							<goal>simian</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<!-- sourcemonitor -->
			<plugin>
				<groupId>com.huawei.ci.maven.plugins</groupId>
				<artifactId>maven-sourcemonitor-plugin</artifactId>
				<version>1.2</version>
				<configuration>
					<excludes>${excludesFile}</excludes>
				</configuration>
				<executions>
					<execution>
						<phase>site</phase>
						<goals>
							<goal>sourcemonitor</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<!-- surefire junit -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.6</version>
				<configuration>
